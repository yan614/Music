<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.music">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ToastUtile"></activity>
        <activity android:name=".MediaUtils" />
        <activity android:name=".MusicListFragment" />
        <activity android:name=".BottomFragment" />
        <activity android:name=".PlayingListLab" />
        <activity android:name=".PlayingList" />
        <activity android:name=".Mp3Lab" />
        <activity android:name=".Mp3Info" />
        <activity android:name=".PlayerMsg" />
        <activity android:name=".MusicMsg" />
        <activity android:name=".MusicAdapter" />

        <service
            android:name=".MusicService"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>